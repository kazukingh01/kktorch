{
    "name": "dino",
    "user_parameters": {
        "___n_layer": 12,
        "___n_dim": 192,
        "___n_head": 3,
        "___dropout_p": 0.0,
        "___patch_size": 16,
        "___img_size": 224,
        "___n_projection": 64
    },
    "network": [
        {
            "class": "ApplyModule",
            "args": {
                "name": "backbone",
                "network": [
                    {
                        "class": "SharedVariablesModule",
                        "args": "{'n_aug': input.shape[1]}"
                    },
                    {
                        "class": "EvalModule",
                        "args": "input.reshape(-1,*input.shape[-3:])"
                    },
                    {
                        "class": "ConfigModule",
                        "args": "../vit/vit.json"
                    },
                    {
                        "class": "EvalModule",
                        "args": "input.reshape(-1, variables['n_aug'], *input.shape[1:])"
                    }
                ]
            }
        },
        {
            "class": "AggregateInput",
            "args": "cat",
            "kwargs": {
                "dim": 1
            }
        },
        {
            "class": "Linear",
            "args": ["___n_dim", "___n_projection"]
        }
    ]
}