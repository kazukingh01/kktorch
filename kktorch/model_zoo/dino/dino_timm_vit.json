{
    "name": "dino",
    "user_parameters": {
        "___n_dim": 192,
        "___n_projection": 128
    },
    "network": [
        {
            "class": "ApplyModule",
            "args": {
                "name": "backbone",
                "network": [
                    {
                        "class": "SharedVariablesModule",
                        "args": "{'n_aug': input.shape[1]}"
                    },
                    {
                        "class": "EvalModule",
                        "args": "input.reshape(-1,*input.shape[-3:])"
                    },
                    {
                        "class": "TimmModule",
                        "args": "vit_tiny_patch16_224",
                        "kwargs": {
                            "pretrained": false,
                            "set_ident_layers": ["head"]
                        }
                    },
                    {
                        "class": "EvalModule",
                        "args": "input.reshape(-1, variables['n_aug'], *input.shape[1:])"
                    }
                ]
            }
        },
        {
            "class": "AggregateInput",
            "args": "cat",
            "kwargs": {
                "dim": 1
            }
        },
        {
            "class": "Linear",
            "args": ["___n_dim", "___n_projection"]
        }
    ]
}