{
    "name": "huggingface",
    "user_parameters": {
        "___n_node": 768,
        "___n_classes": 4,
        "___freeze_layers": [
            "^embeddings",
            "^encoder\\.layer\\.[0-9]\\."
        ]
    },
    "network": [
        {
            "class": "HuggingfaceModule",
            "args": "cl-tohoku/bert-base-japanese-whole-word-masking",
            "kwargs": {
                "freeze_layers": "___freeze_layers"
            }
        },
        {
            "class": "SelectIndexListInput",
            "args": 1
        },
        {
            "class": "Linear",
            "args": ["___n_node", "___n_classes"]
        }
   ]
}