{
    "name": "huggingface",
    "user_parameters": {
        "___n_node": 768,
        "___freeze_layers": [
            "^shared\\.weight",
            "^encoder\\.block\\.[0-9]\\.layer",
            "^decoder\\.block\\.[0-9]\\.layer"
        ]
    },
    "network": [
        {
            "class": "HuggingfaceModule",
            "args": "sonoisa/t5-base-japanese",
            "kwargs": {
                "freeze_layers": "___freeze_layers"
            }
        },
        {
            "class": "SelectIndexListInput",
            "args": 0
        },
        {
            "class": "SharedParameterModule",
            "args": "huggingface.0.model.shared.weight",
            "kwargs": {
                "name": "embedding",
                "is_copy": false,
                "output_eval": "[input, param]"
            }
        },
        {
            "class": "EinsumInput",
            "args": "abc,dc->abd"
        }
   ]
}